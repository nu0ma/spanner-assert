{
  "name": "spanner-assert",
  "version": "0.1.0",
  "description": "SpannerレコードをYAML期待値で検証するためのライブラリ",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "pnpm run clean && tsdown --config tsdown.config.ts",
    "clean": "rm -rf dist",
    "lint": "biome check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:e2e": "pnpm run build && tsx tests/e2e/run.ts"
  },
  "keywords": [
    "spanner",
    "assertion",
    "yaml",
    "gcp"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.7.4",
    "tsdown": "^0.9.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.1"
  },
  "dependencies": {
    "@google-cloud/spanner": "^7.6.1",
    "js-yaml": "^4.1.0"
  },
  "packageManager": "pnpm@10.14.0"
}
