{
  "name": "spanner-assert",
  "version": "0.1.0",
  "description": "Library for validating Cloud Spanner records against YAML expectations",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "pnpm run clean && tsdown --config tsdown.config.ts",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:e2e": "pnpm run build && tsx tests/e2e/run.ts"
  },
  "keywords": [
    "spanner",
    "assertion",
    "yaml",
    "gcp"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.7.4",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-testing-library": "^7.13.2",
    "eslint-plugin-unused-imports": "^4.2.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.9.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.1"
  },
  "dependencies": {
    "@google-cloud/spanner": "^7.6.1",
    "js-yaml": "^4.1.0"
  },
  "packageManager": "pnpm@10.17.0"
}
